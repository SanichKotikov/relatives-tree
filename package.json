{
  "name": "dehimer-relatives-tree",
  "version": "3.2.13",
  "description": "Calculates families and nodes positions for rendering a family tree",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "benchmarks": "tsc --project ./benchmarks/tsconfig.json && node ./benchmarks/index.js",
    "start": "vite",
    "build": "tsc",
    "size": "size-limit",
    "test": "uvu -r tsm __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dehimer/relatives-tree.git"
  },
  "bugs": {
    "url": "https://github.com/dehimer/relatives-tree/issues"
  },
  "homepage": "https://github.com/dehimer/relatives-tree",
  "author": "Denis Novikov <den-vse@yandex.ru> (https://dehimer.github.io)",
  "license": "MIT",
  "files": [
    "lib",
    "src",
    "samples"
  ],
  "keywords": [
    "family-tree",
    "family",
    "relatives",
    "node",
    "connection"
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "9.0.0",
    "benchmark": "2.1.4",
    "prettier": "3.0.3",
    "size-limit": "9.0.0",
    "tsm": "2.3.0",
    "typescript": "5.2.2",
    "uvu": "0.5.6",
    "vite": "4.4.11"
  },
  "size-limit": [
    {
      "path": "lib/index.js"
    }
  ],
  "types": "./lib/index.d.ts",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
